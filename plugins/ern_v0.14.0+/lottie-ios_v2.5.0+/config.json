{
  "ios": {
    "copy": [
      {
        "source": "./lottie-ios",
        "dest": "{{{projectName}}}/Libraries/Lottie"
      },
      {
        "source": "./Lottie.xcodeproj",
        "dest": "{{{projectName}}}/Libraries/Lottie"
      },
      {
        "source": "./Lottie",
        "dest": "{{{projectName}}}/Libraries/Lottie"
      }
    ],
    "pbxproj": {
      "addProject": [
        {
          "path": "Lottie/Lottie.xcodeproj",
          "group": "Libraries",
          "staticLibs": [
            {
              "name": "libLottie.a",
              "target": "LottieLibraryIOS"
            }
          ]
        }
      ],
      "addHeaderSearchPath": [
        "\"$(SRCROOT)/{{{projectName}}}/Libraries/Lottie/lottie-ios/Classes/PublicHeaders/**\""
      ]
    }
  }
}
